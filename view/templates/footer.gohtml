{{ define "footer" }}
      <div class="links">
        <a class="underline" href="/about">About</a>
      </div>

      <div class="version">
        <p>
          <a title="Commit hash" href="https://github.com/tarkov-database/website/commit/{{ .App.CommitLong }}" target="_blank" rel="noopener">{{ .App.CommitShort }}</a> 
          <time title="Build date" datetime="{{ formatTime "RFC3339" .App.BuildDate }}">({{ formatTime "02 Jan 2006" .App.BuildDate }})</time>
        </p>
      </div>
{{ end }}
